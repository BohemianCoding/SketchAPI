{
  "typescript.tsdk": "./node_modules/typescript/lib",
  "search.exclude": {
    "**/node_modules": true,
    "**/build": true,
    "**/.awcache": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/build": true,
    "**/.awcache": true,
    "**/_external": true,
    "**/_reference": true,
    "**/_site": true,
    "**/_vendor": true,
    "**/.bundle": true,
    "**/.sass-cache": true,
    ".jekyll-metadata": true
  },
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "javascript.validate.enable": false,
  "eslint.enable": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode"
}
